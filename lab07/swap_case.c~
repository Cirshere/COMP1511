// Program that swaps the case of the letters in a string of text
// Christopher Hoang, z5259780
// 31/03/2020

#include<stdio.h>

int main (void) {
    int ch = getchar();
    
    while (ch != EOF) {
        if (ch >= 'a' && ch <= 'z') {
            putchar(ch - 32);
        }
        
        else if (ch >= 'A' && ch <= 'Z') {
            putchar(ch + 32);
        }
        else {
            putchar(ch);
        }
        ch = getchar();
        
    }
    
}
